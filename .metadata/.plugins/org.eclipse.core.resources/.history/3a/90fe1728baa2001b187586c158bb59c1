package fundamentos;

public class DesafioAritmeticosResposta {
	public static void main(String[] args) {
		
		/*
		 * 
		 * Minha resolusão do desafio
		 * 
		 */
		// Variaveis
		double mNumA = Math.pow(6 * (3 + 2), 2);
		
		double mSuperiorA = mNumA / (3 * 2);
		double mSuperiorB = Math.pow(((1 - 5) * (2 - 7)) / 2, 2);
		double mSuperior = Math.pow(mSuperiorA - mSuperiorB, 3);

		double mInferior = Math.pow(10, 3);

		double mRresultado = mSuperior / mInferior;

		
		/*
		 * 
		 * Resolução do curso
		 * 
		 */
		
		
		System.out.println("Numero A = " + numA);
		System.out.println("Superior A = " + superiorA);
		System.out.println("Superior B = " + superiorB);
		System.out.println("Superior = " + superior);
		System.out.println("Inferior = " + inferior);
		System.out.println("Resultado = " + resultado);
	}
}
