package fundamentos;

public class DesafioAritmeticosResposta {
	public static void main(String[] args) {
		// Variaveis
		double numA = Math.pow(6 * (3 + 2), 2);
		
		double superiorA = numA / (3 * 2);
		double superiorB = Math.pow(((1 - 5) * (2 - 7)) / 2, 2);
		double superior = Math.pow(superiorA - superiorB, 3);

		double inferior = Math.pow(10, 3);

		double resultado = superior / inferior;

		System.out.println("Numero A = " + numA);
		System.out.println("Superior A = " + superiorA);
		System.out.println("Superior B = " + superiorB);
		System.out.println("Superior = " + superior);
		System.out.println("Inferior = " + inferior);
		System.out.println("Resultado = " + resultado);
	}
}
